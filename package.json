{
  "name": "@eoscostarica/eoscr-components",
  "version": "1.1.0",
  "description": "A collection of React Components for EOSIO",
  "author": "EOS Costa Rica",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eoscostarica/eoscr-components.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@eoscostarica/eoscr-theme": "^1.0.3",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.14",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "clsx": "^1.1.0",
    "cross-env": "^7.0.2",
    "crypto-js": "^4.0.0",
    "eosjs": "^20.0.3",
    "eosjs-api": "^7.0.4",
    "gh-pages": "^3.1.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-google-recaptcha": "^2.0.1",
    "react-identicons": "^1.2.4",
    "react-is": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-scripts": "^3.4.1",
    "react-virtualized": "^9.21.2",
    "sha256": "^0.2.0",
    "styled-components": "^5.1.0",
    "text-encoding": "^0.7.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.2",
    "@storybook/react": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.5",
    "@storybook/theming": "^5.3.19",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "microbundle-crl": "^0.13.10",
    "prettier": "^2.0.4"
  },
  "bugs": {
    "url": "https://github.com/eoscostarica/eoscr-components/issues"
  },
  "homepage": "https://github.com/eoscostarica/eoscr-components#readme"
